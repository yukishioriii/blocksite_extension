{
  "name": "Alcohol blocking time",
  "version": "1.0",
  "manifest_version": 3,
  "description": "Alcohol blocking site.",
  "action": {
    "default_popup": "popup.html",
    "default_title": "Alcohol blocking main"
  },
  "icons": {
    "16": "lance.png",
    "32": "lance.png",
    "48": "lance.png",
    "128": "lance.png"
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*/*"
      ],
      "js": [
        "block.js"
      ]
    }
  ]
}